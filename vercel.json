{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/services/pump-cylinder-repair",
      "destination": "/services/cylinder-repair",
      "permanent": true
    },
    {
      "source": "/services/pump-cylinder-repair/",
      "destination": "/services/cylinder-repair",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/index.html"
    }
  ]
}